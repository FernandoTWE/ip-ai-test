      height: 120px;
      animation: spin 2s linear infinite;
      margin: auto;
    }
    @keyframes spin {
      0% { transform: rotate(0deg); }
      100% { transform: rotate(360deg); }
    }
    .hidden {
      display: none;
    }
    .select-container {
      display: flex;
      justify-content: space-between;
      margin-top: 16px;
    }
    .select-container label {
      margin-right: 8px;
    }
  </style>
